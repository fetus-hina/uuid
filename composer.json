{
    "name": "jp3cki/uuid",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.0",
        "php-64bit": ">= 8.0",
        "ext-pcre": "*",
        "ext-hash": "*"
    },
    "require-dev": {
        "jp3cki/coding-standard": "*",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\uuid\\": "src",
            "jp3cki\\uuid\\test\\": "test"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.0.28"
        }
    }
}
