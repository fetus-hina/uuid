{
    "name": "jp3cki/uuid",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@bouhime.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">= 5.4",
        "ext-pcre": "*",
        "ext-hash": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|5.*",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "suggest": {
        "paragonie/random_compat": "random_bytes() polyfill library for PHP 5.6 or earlier."
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\uuid\\": "src",
            "jp3cki\\uuid\\test\\": "test"
        }
    }
}
