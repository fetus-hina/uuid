{
    "name": "jp3cki/uuid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "php-64bit": ">= 8.2",
        "ext-hash": "*",
        "ext-pcre": "*",
        "paragonie/constant_time_encoding": "^3.1.3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48.2",
        "jp3cki/coding-standard": "^1.0.3",
        "phpstan/phpstan": "^2.1.32",
        "phpunit/phpunit": "^11.5.45",
        "squizlabs/php_codesniffer": "^3.13.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "jp3cki\\uuid\\": "src",
            "jp3cki\\uuid\\test\\": "test"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.2.29"
        }
    }
}
